<script type="text/javascript">
    function getChartOptions(labels, percents){
        return {
            type: 'pie',
            data: {
                labels: labels,
                datasets: [{
                    label: '# %',
                    data: percents,
                    backgroundColor: [
                        'rgba(255, 99, 132, 0.2)',
                        'rgba(54, 162, 235, 0.2)',
                        'rgba(255, 206, 86, 0.2)',
                        'rgba(75, 192, 192, 0.2)',
                        'rgba(153, 102, 255, 0.2)',
                        'rgba(255, 159, 64, 0.2)'
                    ],
                    borderColor: [
                        'rgba(255,99,132,1)',
                        'rgba(54, 162, 235, 1)',
                        'rgba(255, 206, 86, 1)',
                        'rgba(75, 192, 192, 1)',
                        'rgba(153, 102, 255, 1)',
                        'rgba(255, 159, 64, 1)'
                    ],
                    borderWidth: 1
                }]
            },
            options: {
                legend:{
                    position: 'bottom'
                }
            }
        }
    }

    var ctx = document.getElementById("useOfProceedsChart");
    var ctx2 = document.getElementById("tokenAllocation");
    var labels1  = ["Software Development - 45%", "Counseling Boot camps - 25%", "Marketing - 12%",
        "Operational - 8%", "Legal - 5%", "Miscellaneous - 5%"];
    var data1 = [45, 25, 12, 8, 5, 5];
    var labels2 = ["Fundraiser - 50%", "Grants 20%", "Long term Budget 20%", "Team 10%"];
    var data2 = [50,20,20,10];

    var myChart = new Chart(ctx, getChartOptions(labels1,data1));
    var myChart2 = new Chart(ctx2, getChartOptions(labels2, data2));

</script>

